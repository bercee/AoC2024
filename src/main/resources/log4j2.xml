<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">

    <!-- Console Appender -->
    <Appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout pattern="%d{yyyy-MM-dd HH:mm:ss} - %msg%n%throwable"/>
        </Console>
    </Appenders>

    <!-- Loggers -->
    <Loggers>
        <Root level="info">
            <AppenderRef ref="Console"/>
        </Root>
    </Loggers>
</Configuration>
